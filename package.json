{
  "name": "next-starter-website-template",
  "description": "A Next.js starter template for a website",
  "author": "Will McBride <will@magnatome.com>",
  "repository": "https://github.com/magnatome/next-starter-website-template",
  "version": "1.1.0",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "starter",
    "template",
    "website"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "serve": "next start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "lint-fix": "eslint --fix --ext .jsx,.js,.ts,.tsx src/",
    "lint-staged": "lint-staged",
    "pretty-quick": "pretty-quick --staged",
    "test": "jest tests --maxWorkers=10",
    "test:coverage": "jest tests --coverage --maxWorkers=10",
    "test:watch": "jest tests --watch",
    "test:staged": "jest -o --maxWorkers=10",
    "jest": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "dependencies": {
    "next": "^11.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "styled-components": "^5.3.1",
    "swr": "^1.0.0",
    "twin.macro": "^2.7.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-twin": "^1.0.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "jest-next-dynamic": "^1.0.1",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.2",
    "tailwindcss": "^2.2.8"
  }
}
